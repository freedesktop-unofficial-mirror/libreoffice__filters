QNameToString::QNameToString()
{
    init_doctok();
    init_ooxml();
}

void WRITERFILTER_RESOURCEMODEL_DLLPUBLIC analyzerIds()
{
    cout << "<ids type=\"sprm\">" << endl;
    
    sprmidsToXML(cout);
    ooxmlsprmidsToXML(cout);

    cout << "</ids>" << endl;

    cout << "<ids type=\"attribute\">" << endl;
    
    doctokidsToXML(cout);
    ooxmlidsToXML(cout);

    cout << "</ids>" << endl;
}

}
