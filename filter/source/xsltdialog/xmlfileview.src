    PushButton PB_VALIDATE
    {
        Pos					= MAP_APPFONT ( 6 , 6 ) ;
        Size				= MAP_APPFONT ( 60 , 14 ) ;
        TabStop				= TRUE ;
        HelpId				= HID_XML_SOURCE_FILE_VALIDATE;
        Text [ en-US ]	= "~Validate";
    };
};









